export EDITOR="nvim"
export PAGER="less"
export VISUAL="nvim"
export PATH="$HOME/bin:$PATH"
export PATH="$HOME/.local/bin:$PATH"
export PATH="$HOME/scripts:$PATH"
export NVM_DIR="$HOME/.nvm"
[ -s /usr/share/nvm/init-nvm.sh ] && source /usr/share/nvm/init-nvm.sh
export PNPM_HOME="$HOME/.local/share/pnpm"
case ":$PATH:" in
  *":$PNPM_HOME:"*) ;;
  *) export PATH="$PNPM_HOME:$PATH" ;;
esac
export SYRA_BUILD_DIR="/mnt/syra_data/builds"
export SYRA_ASSETS_DIR="/mnt/syra_data/assets"
export SYRA_MODELS_DIR="/mnt/syra_data/models"
export SYRA_CACHE_DIR="/mnt/syra_data/cache"
